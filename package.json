{
  "name": "ng2-plate",
  "scripts": {
    "clean": "npm cache clean && rimraf node_modules coverage dist",
    "clean:dist": "rimraf dist",

    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",

    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack --progress --profile --colors --display-error-details --display-cached",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "webpack --progress --profile --colors --display-error-details --display-cached --bail",

    "server:dev": "webpack-dev-server --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "server:prod": "http-server dist --cors",

    "lint": "eslint 'src/**/*.js'",

    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "e2e": "protractor",
    "e2e-live": "protractor --elementExplorer",
    "test": "karma start",
    "test:watch": "npm run test -- --no-single-run --auto-watch",

    "start": "npm run server:dev",

    "postinstall": "npm run webdriver:update && npm prune"
  },
  "dependencies": {
    "@angular/http": "2.0.0-rc.1",
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/platform-server": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "core-js": "^2.2.2",
    "lodash": "^4.11.1",
    "normalize.css": "^4.1.1",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-angular2-annotations": "^5.1.0",
    "babel-plugin-transform-class-properties": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "colorguard": "^1.2.0",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^2.1.4",
    "css-loader": "^0.23.1",
    "cssnano": "^3.5.2",
    "doiuse": "^2.3.0",
    "es6-promise": "^3.2.1",
    "es6-promise-loader": "^1.0.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import": "^1.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.16.2",
    "http-server": "^0.9.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-spec-reporter": "^2.4.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.3",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "node-sass": "^3.7.0",
    "null-loader": "^0.1.1",
